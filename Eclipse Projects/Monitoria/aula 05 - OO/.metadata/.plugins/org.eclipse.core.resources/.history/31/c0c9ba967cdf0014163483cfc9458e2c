package exercicios;

public class Pessoa {
	private String nome;
	
	//contrutor:
	public Pessoa(String nome){
		this.nome = nome;
	}
	//metodo:
	public void setNome(String nome){
		this.nome = nome;
	}
	//armazenando:
	public String getNome(){
		return this.nome;
	}
	public static void main(String [] args){
	Pessoa pessoa = new Pessoa ("José");
	}
	
	
	
}
