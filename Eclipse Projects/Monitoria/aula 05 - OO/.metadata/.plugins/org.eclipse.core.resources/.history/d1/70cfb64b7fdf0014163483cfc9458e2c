package exercicios;

public class Vaca {
	private String nome;
	private int idade;
	private double peso;
	private int alimento;
	public Vaca(String nome, int idade, double peso, int alimento){
		this.nome = nome;
		this.idade = idade;
		this.peso = peso;
		this.alimento = alimento;
	}
	public String getNome(){
		return nome;
	}
	public void setNome(String nome){
		this.nome = nome;
	}
	public int getIdade(){
		return idade;
	}
	public void setIdade(int idade){
		this.idade = idade;
	}
	

}
