package Exercicios;
import java.util.Scanner;
public class Multiplicacao {
	
	public int Produto (int a, int b){
		int resposta = 0;
		if (a > 0){
			resposta = b + Produto(a-1, b);
		}
		return resposta;
	}
	
	public static void main (String [] args){
		Scanner in = new Scanner (System.in);
		
	}

}
