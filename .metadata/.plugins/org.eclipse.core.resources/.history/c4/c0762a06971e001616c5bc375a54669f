package sockets;

import java.io.ByteArrayOutputStream;
import java.net.DatagramSocket;
import java.net.Socket;
import java.util.Scanner;
public class TCPClient {
/*
 * deve conter:
 * Socket de conexao com servidor
 * array de bytes para transmissao de pacotes
 * input stream
 * file output stream
 * buffered output stream
 * variavel do tipo int para calcular quantos bytes deve ler (arrayBytes, 0, tamanhodo array de bytes)
 * escrever no buffer 
 */
	public static void main(String[] args) throws Exception{
		try
		{
			Socket sock = new Socket("localhost", 5050); //socket com IPServer e portaServer	
			
		}
		catch(Exception e)
		{
			System.out.println("Problema com TCPCliente: " + e.getMessage());
		}
		ByteArrayOutputStream data = new ByteArrayOutputStream();
	}
}
